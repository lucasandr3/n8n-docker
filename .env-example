## Postgres
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgres-n8n
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=postgres
DB_POSTGRESDB_USER=postgres
DB_POSTGRESDB_PASSWORD=postgres
# Para produção, altere estas senhas:
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=postgres

## Redis
QUEUE_BULL_REDIS_HOST=redis-n8n
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_DB=0
QUEUE_BULL_REDIS_PASSWORD=

## N8N - Configuração de URL
# Para desenvolvimento local, use:
# N8N_HOST=localhost
# N8N_PROTOCOL=http
# WEBHOOK_URL=http://localhost:8081/
# N8N_EDITOR_BASE_URL=http://localhost:8081/
# Para produção, use:
# N8N_HOST=n8n.gestgo.com.br
# N8N_PROTOCOL=https
# WEBHOOK_URL=https://n8n.gestgo.com.br/
# N8N_EDITOR_BASE_URL=https://n8n.gestgo.com.br/
N8N_HOST=localhost
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:8081/
N8N_PORT=5678
N8N_EDITOR_BASE_URL=http://localhost:8081/
# Desabilitar telemetria para desenvolvimento local
N8N_DIAGNOSTICS_ENABLED=false
N8N_PERSONALIZATION_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=false
N8N_METRICS=false
EXECUTIONS_DATA_PRUNE=false

## Gerais
N8N_RUNNERS_ENABLED=true
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
N8N_BLOCK_ENV_ACCESS_IN_NODE=true
N8N_FEATURE_FLAG_MCP=true
N8N_GIT_NODE_DISABLE_BARE_REPOS=true
GENERIC_TIMEZONE=America/Sao_Paulo

## Queue
EXECUTIONS_MODE=queue
OFFLOAD_MANUAL_EXECUTIONS_TO_WORKERS=true

## Webhooks
# Secret para autenticação de webhooks (GitHub/GitLab)
# Gere uma string aleatória segura: openssl rand -hex 32
WEBHOOK_SECRET=

## Seguranca
N8N_ENCRYPTION_KEY=

## Evolution API
DATABASE_ENABLED=true
DATABASE_PROVIDER=postgresql
DATABASE_CONNECTION_URI=postgresql://postgres:postgres@postgres-n8n:5432/evolution?schema=public
CACHE_REDIS_URI=redis://redis-n8n:6379/0
AUTHENTICATION_API_KEY=

## Laravel
APP_NAME=Laravel
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://api.gestgo.com.br

DB_CONNECTION=pgsql
DB_HOST=postgres-n8n
